<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#1a1a1a"/>
  <g>
    <!-- ICT Network Diagram -->
    <!-- Cloud -->
    <path d="M400,150 C450,120 500,120 550,150 C580,120 630,120 650,170 C680,180 680,220 650,240 C660,270 630,300 600,290 C570,320 510,320 480,290 C450,310 400,300 380,270 C350,280 320,260 330,230 C310,210 310,170 340,150 C360,120 380,130 400,150" 
          fill="#333" stroke="#555" stroke-width="2" />
    <text x="490" y="210" text-anchor="middle" font-family="Arial" font-size="20" fill="#ccc">Cloud</text>
    
    <!-- Server Rack -->
    <rect x="150" y="250" width="100" height="200" fill="#333" stroke="#555" stroke-width="2" />
    <rect x="160" y="270" width="80" height="20" fill="#444" stroke="#666" stroke-width="1" />
    <rect x="160" y="300" width="80" height="20" fill="#444" stroke="#666" stroke-width="1" />
    <rect x="160" y="330" width="80" height="20" fill="#444" stroke="#666" stroke-width="1" />
    <rect x="160" y="360" width="80" height="20" fill="#444" stroke="#666" stroke-width="1" />
    <rect x="160" y="390" width="80" height="20" fill="#444" stroke="#666" stroke-width="1" />
    <rect x="160" y="420" width="80" height="20" fill="#444" stroke="#666" stroke-width="1" />
    <text x="200" y="240" text-anchor="middle" font-family="Arial" font-size="16" fill="#ccc">Servers</text>
    
    <!-- Network Switch -->
    <rect x="350" y="350" width="150" height="40" fill="#333" stroke="#555" stroke-width="2" />
    <circle cx="370" cy="370" r="5" fill="#0f0" />
    <circle cx="390" cy="370" r="5" fill="#0f0" />
    <circle cx="410" cy="370" r="5" fill="#0f0" />
    <circle cx="430" cy="370" r="5" fill="#0f0" />
    <circle cx="450" cy="370" r="5" fill="#0f0" />
    <circle cx="470" cy="370" r="5" fill="#0f0" />
    <text x="425" y="340" text-anchor="middle" font-family="Arial" font-size="16" fill="#ccc">Network Switch</text>
    
    <!-- Workstations -->
    <g>
      <!-- Workstation 1 -->
      <rect x="550" y="300" width="60" height="40" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="560" y="280" width="40" height="30" fill="#444" stroke="#666" stroke-width="1" />
      <text x="580" y="360" text-anchor="middle" font-family="Arial" font-size="12" fill="#ccc">PC 1</text>
      
      <!-- Workstation 2 -->
      <rect x="550" y="400" width="60" height="40" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="560" y="380" width="40" height="30" fill="#444" stroke="#666" stroke-width="1" />
      <text x="580" y="460" text-anchor="middle" font-family="Arial" font-size="12" fill="#ccc">PC 2</text>
      
      <!-- Workstation 3 -->
      <rect x="650" y="300" width="60" height="40" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="660" y="280" width="40" height="30" fill="#444" stroke="#666" stroke-width="1" />
      <text x="680" y="360" text-anchor="middle" font-family="Arial" font-size="12" fill="#ccc">PC 3</text>
      
      <!-- Workstation 4 -->
      <rect x="650" y="400" width="60" height="40" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="660" y="380" width="40" height="30" fill="#444" stroke="#666" stroke-width="1" />
      <text x="680" y="460" text-anchor="middle" font-family="Arial" font-size="12" fill="#ccc">PC 4</text>
    </g>
    
    <!-- Connection Lines -->
    <g stroke="#666" stroke-width="2" stroke-dasharray="5,5" fill="none">
      <!-- Cloud to Server -->
      <line x1="400" y1="230" x2="200" y2="270" />
      
      <!-- Server to Switch -->
      <line x1="200" y1="350" x2="350" y2="370" />
      
      <!-- Switch to Workstations -->
      <line x1="500" y1="350" x2="550" y2="320" />
      <line x1="500" y1="370" x2="550" y2="420" />
      <line x1="500" y1="360" x2="650" y2="320" />
      <line x1="500" y1="380" x2="650" y2="420" />
    </g>
    
    <!-- Text Label -->
    <text x="400" y="520" text-anchor="middle" font-family="Arial" font-size="24" fill="#ffffff">ICT Network Infrastructure</text>
  </g>
</svg>