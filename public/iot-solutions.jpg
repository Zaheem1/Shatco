<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#1a1a1a"/>
  <g>
    <!-- IoT Network Diagram -->
    <!-- Central Hub/Cloud -->
    <circle cx="400" cy="200" r="80" fill="#333" stroke="#555" stroke-width="2" />
    <text x="400" y="205" text-anchor="middle" font-family="Arial" font-size="20" fill="#ccc">IoT Cloud</text>
    
    <!-- Connected Devices -->
    <!-- Smart Home -->
    <g transform="translate(150, 350)">
      <rect x="0" y="0" width="100" height="80" fill="#333" stroke="#555" stroke-width="2" />
      <polygon points="0,0 50,-30 100,0" fill="#444" stroke="#555" stroke-width="2" />
      <rect x="40" y="40" width="20" height="40" fill="#444" stroke="#555" stroke-width="1" />
      <rect x="20" y="20" width="15" height="15" fill="#666" />
      <rect x="65" y="20" width="15" height="15" fill="#666" />
      <text x="50" y="100" text-anchor="middle" font-family="Arial" font-size="14" fill="#ccc">Smart Home</text>
    </g>
    
    <!-- Industrial Sensors -->
    <g transform="translate(300, 400)">
      <circle cx="0" cy="0" r="30" fill="#333" stroke="#555" stroke-width="2" />
      <circle cx="0" cy="0" r="20" fill="#444" stroke="#555" stroke-width="1" />
      <circle cx="0" cy="0" r="10" fill="#666" stroke="#555" stroke-width="1" />
      <line x1="-40" y1="-40" x2="-20" y2="-20" stroke="#666" stroke-width="3" />
      <line x1="40" y1="-40" x2="20" y2="-20" stroke="#666" stroke-width="3" />
      <line x1="-40" y1="40" x2="-20" y2="20" stroke="#666" stroke-width="3" />
      <line x1="40" y1="40" x2="20" y2="20" stroke="#666" stroke-width="3" />
      <text x="0" y="50" text-anchor="middle" font-family="Arial" font-size="14" fill="#ccc">Sensors</text>
    </g>
    
    <!-- Smart City -->
    <g transform="translate(500, 350)">
      <rect x="0" y="0" width="20" height="80" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="30" y="20" width="20" height="60" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="60" y="0" width="20" height="80" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="90" y="30" width="20" height="50" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="120" y="10" width="20" height="70" fill="#333" stroke="#555" stroke-width="2" />
      <circle cx="70" cy="-20" r="10" fill="#666" />
      <text x="70" y="100" text-anchor="middle" font-family="Arial" font-size="14" fill="#ccc">Smart City</text>
    </g>
    
    <!-- Wearable Device -->
    <g transform="translate(650, 400)">
      <rect x="0" y="0" width="40" height="60" rx="5" ry="5" fill="#333" stroke="#555" stroke-width="2" />
      <rect x="5" y="5" width="30" height="40" fill="#444" stroke="#555" stroke-width="1" />
      <circle cx="20" cy="55" r="3" fill="#666" />
      <text x="20" y="80" text-anchor="middle" font-family="Arial" font-size="14" fill="#ccc">Wearable</text>
    </g>
    
    <!-- Connection Lines -->
    <g stroke="#4a6b8a" stroke-width="2" stroke-dasharray="5,5" fill="none">
      <!-- Cloud to Devices -->
      <line x1="350" y1="250" x2="200" y2="350" />
      <line x1="400" y1="280" x2="300" y2="400" />
      <line x1="450" y1="250" x2="550" y2="350" />
      <line x1="470" y1="230" x2="650" y2="400" />
    </g>
    
    <!-- Data Flow Indicators -->
    <g fill="#3498db">
      <circle cx="275" cy="300" r="5">
        <animate attributeName="opacity" values="1;0.3;1" dur="2s" repeatCount="indefinite" />
      </circle>
      <circle cx="350" cy="340" r="5">
        <animate attributeName="opacity" values="1;0.3;1" dur="1.5s" repeatCount="indefinite" />
      </circle>
      <circle cx="500" cy="300" r="5">
        <animate attributeName="opacity" values="1;0.3;1" dur="2.2s" repeatCount="indefinite" />
      </circle>
      <circle cx="560" cy="315" r="5">
        <animate attributeName="opacity" values="1;0.3;1" dur="1.8s" repeatCount="indefinite" />
      </circle>
    </g>
    
    <!-- Text Label -->
    <text x="400" y="520" text-anchor="middle" font-family="Arial" font-size="24" fill="#ffffff">Internet of Things Solutions</text>
  </g>
</svg>