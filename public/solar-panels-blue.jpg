<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#0a1a2a"/>
  <g>
    <!-- Solar Panel Array -->
    <g transform="translate(100, 100)">
      <!-- Row 1 -->
      <rect x="0" y="0" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      <rect x="110" y="0" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      <rect x="220" y="0" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      <rect x="330" y="0" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      <rect x="440" y="0" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      
      <!-- Row 2 -->
      <rect x="0" y="90" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      <rect x="110" y="90" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      <rect x="220" y="90" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      <rect x="330" y="90" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      <rect x="440" y="90" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      
      <!-- Row 3 -->
      <rect x="0" y="180" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      <rect x="110" y="180" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      <rect x="220" y="180" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      <rect x="330" y="180" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      <rect x="440" y="180" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      
      <!-- Row 4 -->
      <rect x="0" y="270" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      <rect x="110" y="270" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      <rect x="220" y="270" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
      <rect x="330" y="270" width="100" height="80" fill="#3498db" stroke="#2980b9" stroke-width="2" />
      <rect x="440" y="270" width="100" height="80" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" />
    </g>
    
    <!-- Grid Lines -->
    <g stroke="#4a6b8a" stroke-width="1" opacity="0.5">
      <!-- Horizontal grid lines for panels -->
      <line x1="100" y1="120" x2="640" y2="120" />
      <line x1="100" y1="140" x2="640" y2="140" />
      <line x1="100" y1="160" x2="640" y2="160" />
      <line x1="100" y1="210" x2="640" y2="210" />
      <line x1="100" y1="230" x2="640" y2="230" />
      <line x1="100" y1="250" x2="640" y2="250" />
      <line x1="100" y1="300" x2="640" y2="300" />
      <line x1="100" y1="320" x2="640" y2="320" />
      <line x1="100" y1="340" x2="640" y2="340" />
      <line x1="100" y1="390" x2="640" y2="390" />
      <line x1="100" y1="410" x2="640" y2="410" />
      <line x1="100" y1="430" x2="640" y2="430" />
      
      <!-- Vertical grid lines for panels -->
      <line x1="130" y1="100" x2="130" y2="450" />
      <line x1="150" y1="100" x2="150" y2="450" />
      <line x1="170" y1="100" x2="170" y2="450" />
      <line x1="240" y1="100" x2="240" y2="450" />
      <line x1="260" y1="100" x2="260" y2="450" />
      <line x1="280" y1="100" x2="280" y2="450" />
      <line x1="350" y1="100" x2="350" y2="450" />
      <line x1="370" y1="100" x2="370" y2="450" />
      <line x1="390" y1="100" x2="390" y2="450" />
      <line x1="460" y1="100" x2="460" y2="450" />
      <line x1="480" y1="100" x2="480" y2="450" />
      <line x1="500" y1="100" x2="500" y2="450" />
      <line x1="570" y1="100" x2="570" y2="450" />
      <line x1="590" y1="100" x2="590" y2="450" />
      <line x1="610" y1="100" x2="610" y2="450" />
    </g>
    
    <!-- Reflection Highlights -->
    <g fill="#ffffff" opacity="0.3">
      <polygon points="120,110 150,110 130,140" />
      <polygon points="230,110 260,110 240,140" />
      <polygon points="340,110 370,110 350,140" />
      <polygon points="450,110 480,110 460,140" />
      <polygon points="560,110 590,110 570,140" />
      
      <polygon points="120,200 150,200 130,230" />
      <polygon points="230,200 260,200 240,230" />
      <polygon points="340,200 370,200 350,230" />
      <polygon points="450,200 480,200 460,230" />
      <polygon points="560,200 590,200 570,230" />
      
      <polygon points="120,290 150,290 130,320" />
      <polygon points="230,290 260,290 240,320" />
      <polygon points="340,290 370,290 350,320" />
      <polygon points="450,290 480,290 460,320" />
      <polygon points="560,290 590,290 570,320" />
      
      <polygon points="120,380 150,380 130,410" />
      <polygon points="230,380 260,380 240,410" />
      <polygon points="340,380 370,380 350,410" />
      <polygon points="450,380 480,380 460,410" />
      <polygon points="560,380 590,380 570,410" />
    </g>
    
    <!-- Text Label -->
    <text x="400" y="520" text-anchor="middle" font-family="Arial" font-size="24" fill="#ffffff">White and Blue Solar Panels</text>
  </g>
</svg>