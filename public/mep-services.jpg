<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#1a1a1a"/>
  <g>
    <!-- MEP System Diagram -->
    <!-- Building Outline -->
    <rect x="150" y="100" width="500" height="350" fill="none" stroke="#555" stroke-width="2" />
    
    <!-- Floors -->
    <line x1="150" y1="200" x2="650" y2="200" stroke="#555" stroke-width="1" />
    <line x1="150" y1="300" x2="650" y2="300" stroke="#555" stroke-width="1" />
    <line x1="150" y1="400" x2="650" y2="400" stroke="#555" stroke-width="1" />
    
    <!-- Mechanical Systems (HVAC) -->
    <g fill="#3498db" opacity="0.7">
      <!-- Ducts -->
      <rect x="200" y="110" width="400" height="10" />
      <rect x="200" y="210" width="400" height="10" />
      <rect x="200" y="310" width="400" height="10" />
      <rect x="200" y="410" width="400" height="10" />
      
      <!-- Vertical Shaft -->
      <rect x="300" y="110" width="20" height="340" />
      <rect x="500" y="110" width="20" height="340" />
      
      <!-- HVAC Units -->
      <rect x="290" y="130" width="40" height="30" stroke="#2980b9" stroke-width="1" />
      <rect x="290" y="230" width="40" height="30" stroke="#2980b9" stroke-width="1" />
      <rect x="290" y="330" width="40" height="30" stroke="#2980b9" stroke-width="1" />
      
      <rect x="490" y="130" width="40" height="30" stroke="#2980b9" stroke-width="1" />
      <rect x="490" y="230" width="40" height="30" stroke="#2980b9" stroke-width="1" />
      <rect x="490" y="330" width="40" height="30" stroke="#2980b9" stroke-width="1" />
    </g>
    
    <!-- Electrical Systems -->
    <g stroke="#f1c40f" stroke-width="2" fill="none">
      <!-- Main Power Line -->
      <line x1="150" y1="150" x2="650" y2="150" />
      <line x1="150" y1="250" x2="650" y2="250" />
      <line x1="150" y1="350" x2="650" y2="350" />
      
      <!-- Vertical Power Lines -->
      <line x1="200" y1="100" x2="200" y2="450" />
      <line x1="400" y1="100" x2="400" y2="450" />
      <line x1="600" y1="100" x2="600" y2="450" />
      
      <!-- Electrical Panels -->
      <rect x="190" y="170" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
      <rect x="190" y="270" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
      <rect x="190" y="370" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
      
      <rect x="390" y="170" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
      <rect x="390" y="270" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
      <rect x="390" y="370" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
      
      <rect x="590" y="170" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
      <rect x="590" y="270" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
      <rect x="590" y="370" width="20" height="20" stroke="#f1c40f" stroke-width="1" fill="#333" />
    </g>
    
    <!-- Plumbing Systems -->
    <g stroke="#2ecc71" stroke-width="2" fill="none">
      <!-- Main Water Lines -->
      <line x1="180" y1="120" x2="620" y2="120" stroke-dasharray="5,5" />
      <line x1="180" y1="220" x2="620" y2="220" stroke-dasharray="5,5" />
      <line x1="180" y1="320" x2="620" y2="320" stroke-dasharray="5,5" />
      <line x1="180" y1="420" x2="620" y2="420" stroke-dasharray="5,5" />
      
      <!-- Vertical Water Lines -->
      <line x1="250" y1="100" x2="250" y2="450" stroke-dasharray="5,5" />
      <line x1="450" y1="100" x2="450" y2="450" stroke-dasharray="5,5" />
      <line x1="550" y1="100" x2="550" y2="450" stroke-dasharray="5,5" />
      
      <!-- Plumbing Fixtures -->
      <circle cx="250" cy="150" r="8" fill="#2ecc71" />
      <circle cx="250" cy="250" r="8" fill="#2ecc71" />
      <circle cx="250" cy="350" r="8" fill="#2ecc71" />
      
      <circle cx="450" cy="150" r="8" fill="#2ecc71" />
      <circle cx="450" cy="250" r="8" fill="#2ecc71" />
      <circle cx="450" cy="350" r="8" fill="#2ecc71" />
      
      <circle cx="550" cy="150" r="8" fill="#2ecc71" />
      <circle cx="550" cy="250" r="8" fill="#2ecc71" />
      <circle cx="550" cy="350" r="8" fill="#2ecc71" />
    </g>
    
    <!-- Legend -->
    <g transform="translate(150, 470)">
      <!-- Mechanical -->
      <rect x="0" y="0" width="20" height="10" fill="#3498db" opacity="0.7" />
      <text x="30" y="10" font-family="Arial" font-size="12" fill="#ccc">Mechanical</text>
      
      <!-- Electrical -->
      <line x1="100" y1="5" x2="120" y2="5" stroke="#f1c40f" stroke-width="2" />
      <text x="130" y="10" font-family="Arial" font-size="12" fill="#ccc">Electrical</text>
      
      <!-- Plumbing -->
      <line x1="200" y1="5" x2="220" y2="5" stroke="#2ecc71" stroke-width="2" stroke-dasharray="5,5" />
      <text x="230" y="10" font-family="Arial" font-size="12" fill="#ccc">Plumbing</text>
    </g>
    
    <!-- Text Label -->
    <text x="400" y="520" text-anchor="middle" font-family="Arial" font-size="24" fill="#ffffff">MEP Systems</text>
  </g>
</svg>