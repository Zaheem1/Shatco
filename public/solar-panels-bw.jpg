<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#1a1a1a"/>
  <g fill="#ffffff" fill-opacity="0.8">
    <!-- Solar Panel Array -->
    <g transform="translate(100, 100)">
      <!-- Row 1 -->
      <rect x="0" y="0" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="110" y="0" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="220" y="0" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="330" y="0" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="440" y="0" width="100" height="80" stroke="#444" stroke-width="2" />
      
      <!-- Row 2 -->
      <rect x="0" y="90" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="110" y="90" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="220" y="90" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="330" y="90" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="440" y="90" width="100" height="80" stroke="#444" stroke-width="2" />
      
      <!-- Row 3 -->
      <rect x="0" y="180" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="110" y="180" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="220" y="180" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="330" y="180" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="440" y="180" width="100" height="80" stroke="#444" stroke-width="2" />
      
      <!-- Row 4 -->
      <rect x="0" y="270" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="110" y="270" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="220" y="270" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="330" y="270" width="100" height="80" stroke="#444" stroke-width="2" />
      <rect x="440" y="270" width="100" height="80" stroke="#444" stroke-width="2" />
    </g>
    
    <!-- Grid Lines -->
    <g stroke="#555" stroke-width="1" opacity="0.5">
      <!-- Horizontal grid lines for panels -->
      <line x1="100" y1="120" x2="640" y2="120" />
      <line x1="100" y1="140" x2="640" y2="140" />
      <line x1="100" y1="160" x2="640" y2="160" />
      <line x1="100" y1="210" x2="640" y2="210" />
      <line x1="100" y1="230" x2="640" y2="230" />
      <line x1="100" y1="250" x2="640" y2="250" />
      <line x1="100" y1="300" x2="640" y2="300" />
      <line x1="100" y1="320" x2="640" y2="320" />
      <line x1="100" y1="340" x2="640" y2="340" />
      <line x1="100" y1="390" x2="640" y2="390" />
      <line x1="100" y1="410" x2="640" y2="410" />
      <line x1="100" y1="430" x2="640" y2="430" />
      
      <!-- Vertical grid lines for panels -->
      <line x1="130" y1="100" x2="130" y2="450" />
      <line x1="150" y1="100" x2="150" y2="450" />
      <line x1="170" y1="100" x2="170" y2="450" />
      <line x1="240" y1="100" x2="240" y2="450" />
      <line x1="260" y1="100" x2="260" y2="450" />
      <line x1="280" y1="100" x2="280" y2="450" />
      <line x1="350" y1="100" x2="350" y2="450" />
      <line x1="370" y1="100" x2="370" y2="450" />
      <line x1="390" y1="100" x2="390" y2="450" />
      <line x1="460" y1="100" x2="460" y2="450" />
      <line x1="480" y1="100" x2="480" y2="450" />
      <line x1="500" y1="100" x2="500" y2="450" />
      <line x1="570" y1="100" x2="570" y2="450" />
      <line x1="590" y1="100" x2="590" y2="450" />
      <line x1="610" y1="100" x2="610" y2="450" />
    </g>
    
    <!-- Text Label -->
    <text x="400" y="520" text-anchor="middle" font-family="Arial" font-size="24" fill="#ffffff">Solar Panel Array</text>
  </g>
</svg>